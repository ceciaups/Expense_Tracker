@model IEnumerable<Expense_Tracker.Models.CategoryDto>
@{
    ViewBag.Title = "Category";
}

<div>
    <a class="navlink" href="/Category/New">New Category</a>
</div>

<div>
    <h1>Category List</h1>
</div>
<table>
    <tr>
        <th>Category</th>
        <th></th>
        <th></th>
    </tr>
    @foreach (var category in Model)
    {
        <tr>
            <td>@category.CategoryName</td>
            <td><a href="/Category/Edit/@category.CategoryID">Edit</td>
            <td><a href="/Category/DeleteConfirm/@category.CategoryID">Delete</td>
        </tr>
    }
</table>